{
	"name": "@prisma/client",
	"version": "2.0.0-preview021",
	"main": "index.js",
	"types": "index.d.ts",
	"license": "Apache-2.0",
	"scripts": {
		"tsc": "tsc -d",
		"build": "pnpm run cleanup && pnpm run tsc && pnpm run ncc:runtime && pnpm run ncc:generator && cp scripts/backup-index.js index.js &&  cp scripts/any-index.js runtime/index.d.ts",
		"test": "jest",
		"download": "node scripts/download.js",
		"generate": "node scripts/postinstall.js",
		"postinstall": "node scripts/postinstall.js",
		"precommit": "lint-staged && jest",
		"ncc:runtime": "node scripts/build-runtime.js",
		"cleanup": "rm -f index.d.ts && cp scripts/backup-index.js index.js",
		"ncc:generator": "rm -rf generator-build && ncc build dist/generator.js -o generator-build && sed -i.bak 's/env ts-node/env node --max-old-space-size=2048/g' 'generator-build/index.js' && rm -f generator-build/index.js.bak && chmod +x generator-build/index.js",
		"prepare": "pnpm run cleanup && cp scripts/backup-index.js index.js && node scripts/download.js || echo \"\"",
		"prepublishOnly": "pnpm run build"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"tslint --project tsconfig.json -c tslint.json --fix",
			"prettier --write",
			"git add"
		]
	},
	"prisma": {
		"version": "latest"
	},
	"files": [
		"runtime/*.js",
		"runtime/*.d.ts",
		"runtime/**/*.js",
		"runtime/**/*.d.ts",
		"runtime/utils/*",
		"runtime/dist/*",
		"dist",
		"scripts",
		"generator-build",
		"index.js"
	],
	"devDependencies": {
		"@apexearth/copy": "^1.4.4",
		"@prisma/engine-core": "2.0.41",
		"@prisma/fetch-engine": "0.3.34",
		"@prisma/generator-helper": "0.0.29",
		"@prisma/get-platform": "0.1.23",
		"@prisma/sdk": "0.0.73",
		"@types/debug": "^4.1.5",
		"@types/fs-extra": "^8.0.1",
		"@types/jest": "^24.9.0",
		"@types/js-levenshtein": "^1.1.0",
		"@types/node": "^12.12.25",
		"@types/node-fetch": "^2.5.4",
		"@zeit/ncc": "^0.20.5",
		"arg": "^4.1.2",
		"benchmark": "^2.1.4",
		"chalk": "^3.0.0",
		"del": "5.1.0",
		"execa": "3.3.0",
		"flat-map-polyfill": "^0.3.8",
		"fs-monkey": "^0.3.3",
		"get-own-enumerable-property-symbols": "^3.0.2",
		"hasha": "^5.1.0",
		"indent-string": "^4.0.0",
		"is-obj": "^2.0.0",
		"is-regexp": "^2.1.0",
		"jest": "^24.9.0",
		"js-levenshtein": "^1.1.6",
		"lint-staged": "^9.5.0",
		"make-dir": "^3.0.0",
		"node-fetch": "^2.6.0",
		"parcel": "^2.0.0-alpha.3.2",
		"pluralize": "^8.0.0",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.0",
		"set-value": "^3.0.1",
		"stacktrace-parser": "0.1.7",
		"strip-ansi": "^6.0.0",
		"strip-indent": "^3.0.0",
		"ts-jest": "^24.3.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.6.2",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.7.5",
		"webpack": "5.0.0-alpha.14",
		"webpack-cli": "4.0.0-alpha-4"
	},
	"peerDependencies": {
		"prisma2": "*"
	},
	"peerDependenciesMeta": {
		"prisma2": {
			"optional": true
		}
	}
}
