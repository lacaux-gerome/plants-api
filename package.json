{
  "name": "plants-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate:prisma": "prisma2 generate",
    "generate": "yarn run generate:prisma",
    "migrate:database": "yarn prisma migrate save --experimental && yarn prisma migrate up --experimental",
    "build": "yarn run generate && tsc",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "clean": "rm -rf dist",
    "tsc": "yarn tsc"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.2",
    "@types/node": "12.12.35",
    "cross-env": "^7.0.0",
    "graphql-tag": "^2.10.3",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.15",
    "@prisma/client": "2.0.0-beta.2",
    "@types/node-fetch": "2.5.5",
    "apollo-server": "^2.12.0",
    "graphql": "14.6.0",
    "graphql-import-node": "^0.0.4",
    "graphql-toolkit": "^0.7.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
